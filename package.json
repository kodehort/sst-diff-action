{
  "name": "sst-diff-action",
  "version": "0.2.1",
  "description": "Github action that will compile an sst diff and write it to the action summary.",
  "keywords": [
    "actions",
    "diff",
    "sst"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodehort/sst-diff-action.git"
  },
  "license": "MIT",
  "author": "Kodehort Limited",
  "main": "lib/main.js",
  "scripts": {
    "act": "act -j test -W .github/workflows/ci.yml -s GITHUB_TOKEN=$(gh auth token)",
    "build": "tsup",
    "bump": "node ./scripts/bump.js",
    "clean": "git filter-branch --index-filter 'git rm -rf --cached --ignore-unmatch dist' HEAD",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "release": "node ./scripts/release.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@aws-cdk/cloud-assembly-schema": "^2.85.0",
    "@aws-cdk/cloudformation-diff": "^2.85.0",
    "@aws-sdk/client-cloudformation": "^3.358.0",
    "aws-cdk-lib": "^2.85.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@kodehort/eslint-config": "^2.2.1",
    "@kodehort/prettier-config": "^1.0.1",
    "@total-typescript/ts-reset": "^0.5.0",
    "@tsconfig/node16": "^16.1.0",
    "@types/node": "^20.4.1",
    "eslint": "^8.44.0",
    "eslint-plugin-github": "^4.8.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
